<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fun.fleamarket.mapper.DownloadMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="fun.fleamarket.entity.Download">
        <id column="download_id" property="downloadId" />
        <result column="title" property="title" />
        <result column="description" property="description" />
        <result column="created_time" property="createdTime" />
        <result column="updated_time" property="updatedTime" />
        <result column="download_url" property="downloadUrl" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        download_id, title, `description`, created_time, updated_time, download_url
    </sql>

</mapper>
